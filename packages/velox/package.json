{
  "name": "@remyyy/velox",
  "version": "0.0.6",
  "author": "TheRemyyy",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheRemyyy/velox-framework.git",
    "directory": "packages/velox"
  },
  "keywords": [
    "framework",
    "reactivity",
    "signals",
    "performance",
    "zero-copy"
  ],
  "homepage": "https://velox-framework.vercel.app",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "description": "The fastest web framework in the universe",
  "main": "./dist/velox.cjs",
  "module": "./dist/velox.js",
  "types": "./dist/velox.d.ts",
  "exports": {
    ".": {
      "import": "./dist/velox.js",
      "require": "./dist/velox.cjs",
      "types": "./dist/velox.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.js",
      "require": "./dist/jsx-dev-runtime.cjs",
      "types": "./dist/jsx-dev-runtime.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && tsc",
    "test": "vitest"
  },
  "license": "MIT",
  "devDependencies": {
    "jsdom": "^27.3.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4"
  }
}